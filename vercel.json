{
  "buildCommand": "cd ippo-zutsu-app && npm run build",
  "outputDirectory": "ippo-zutsu-app/dist",
  "devCommand": "cd ippo-zutsu-app && npm run dev",
  "installCommand": "cd ippo-zutsu-app && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 